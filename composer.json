{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
        "cartalyst/sentry": "2.1.*",
        "laravelbook/ardent": "dev-master",
        "pda/pheanstalk": "v2.1.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
            "app/controllers/auth",
            "app/controllers/property",
            "app/controllers/service",
			"app/models",
            "app/models/datalogic",
            "app/models/crawler",
            "app/models/crawler/abstracts",
            "app/models/crawler/commands",
            "app/models/crawler/factories",
            "app/models/crawler/scrape",
            "app/models/entities",
            "app/models/exceptions",
            "app/models/interfaces",
            "app/models/repositories",
            "app/models/utility",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
